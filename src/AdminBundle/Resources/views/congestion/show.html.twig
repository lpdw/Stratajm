{% extends 'AdminBundle::admin-layout.html.twig' %}

{% block body %}
    <h1>Congestion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ congestion.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ congestion.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_congestion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_congestion_edit', { 'id': congestion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
